@model List<ActionResult>
@{
  ViewData["Title"] = "Pages";
}
<style>
  .grid{
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 10px;
    padding: 10px;
  }
  .item1 {
    grid-column: 1 / 3;
  }
  .h-1\.0{
    height: 100%;
  }
</style>
<h1>Dashboard</h1>
<div class="grid">
  @Html.Partial("ChartNumber")
  @Html.Partial("ChartContribute")
  <div class="item1">
    @Html.Partial("ChartPercent")
  </div>
</div>
</div>



