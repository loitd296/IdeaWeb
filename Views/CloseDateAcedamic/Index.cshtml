@model IEnumerable<IdeaWeb.Models.CloseDateAcedamic>

@{
    ViewData["Title"] = "Index";
}



<p style="margin-bottom: 15px;">
    <a asp-action="Create" class="button small green --jb-modal">Create New</a>
</p>
<div class="card has-table">
      <header class="card-header">
        <p class="card-header-title">
          <span class="icon"><i class="mdi mdi-account-multiple"></i></span>
          Close Date Acedamic
        </p>
        <a href="#" class="card-header-icon">
          <span class="icon"><i class="mdi mdi-reload"></i></span>
        </a>
      </header>
      <div class="card-content">
<table>
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CloseDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CloseDatePostIdea)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CloseDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CloseDatePostIdea)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id" class="button small blue --jb-modal">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id" class="button small blue --jb-modal">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id" class="button small red --jb-modal">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
